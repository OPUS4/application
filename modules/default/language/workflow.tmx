<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tmx SYSTEM "http://www.gala-global.org/oscarStandards/tmx/tmx14.dtd">
<tmx version="1.4">

 <header creationtoolversion="1.0.0" datatype="winres" segtype="sentence" adminlang="en-us" srclang="de-de" o-tmf="abc" creationtool="XYZTool">
 </header>

  <body>

    <tu tuid="admin_workflow_restricted">
      <tuv xml:lang="en">
        <seg>Restrict document</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Zugriff beschränken</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_restricted_sure">
      <tuv xml:lang="en">
        <seg>Are you sure you want to change the state of the document to 'restricted'?</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Sind Sie sicher, dass Sie das Dokument für beschränkten Zugang freigeben wollen?</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_inprogress">
      <tuv xml:lang="en">
        <seg>Change state: In Progress</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Status ändern: In Bearbeitung</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_inprogress_sure">
      <tuv xml:lang="en">
        <seg>Are you sure you want to change the state of the document to 'inprogress'?</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Sind Sie sicher, dass Sie den Zustand des Dokumentes auf 'In Bearbeitung' setzen wollen?</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_audited">
      <tuv xml:lang="en">
        <seg>Change state: Audited</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Status ändern: Geprüft</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_audited_sure">
      <tuv xml:lang="en">
        <seg>Are you sure you want to change the state of the document to 'audited'?</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Sind Sie sicher, dass Sie den Zustand des Dokumentes auf 'Geprüft' setzen wollen?</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_deleted">
      <tuv xml:lang="en">
        <seg>Delete document</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Dokument löschen</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_deleted_sure">
      <tuv xml:lang="en">
        <seg>Are you sure you want to delete document %1$s?</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Sind Sie sicher, dass Dokument %1$s gelöscht werden soll?</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_removed">
      <tuv xml:lang="en">
        <seg>Delete document permanently</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Dokument komplett löschen</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_removed_sure">
      <tuv xml:lang="en">
        <seg>Are you sure you want to delete document %1$s permanently? All contents will be removed from disk without the possibility to undelete!</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Sind Sie sicher, dass Dokument %1$s komplett gelöscht werden soll? Alle Daten zum Dokument werden vollständig entfernt und es gibt keine Möglichkeit, dies rückgängig zu machen!</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_published">
      <tuv xml:lang="en">
        <seg>Publish document</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Dokument freischalten</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_published_sure">
      <tuv xml:lang="en">
        <seg>Are you sure you want to publish document %1$s?</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Sind Sie sicher, daß Dokument %1$s freigeschaltet werden soll?</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_unpublished">
      <tuv xml:lang="en">
        <seg>Unpublish document</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Dokument zurückziehen</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_unpublished_sure">
      <tuv xml:lang="en">
        <seg>Are you sure you want to unpublish document %1$s?</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Sind Sie sicher, dass Dokument %1$s zurück gezogen werden soll?</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_error_alreadyinstate">
      <tuv xml:lang="en">
        <seg>The document is already in state '%1$s'.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Das Dokument befindet sich bereits in Status '%1$s'.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_error_illegal_transition">
      <tuv xml:lang="en">
        <seg>The document status cannot be changed to '%1$s'.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Der Status des Dokumentes kann nicht zu '%1$s' geändert werden.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_error_already_published">
      <tuv xml:lang="en">
        <seg>The document has already been published.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Das Dokument wurde bereits veröffentlicht.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_error_already_unpublished">
      <tuv xml:lang="en">
        <seg>The document has already been unpublished.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Das Dokument wurde bereits zurück gezogen.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_error_already_deleted">
      <tuv xml:lang="en">
        <seg>Document has already been deleted.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Das Dokument wurde bereits gelöscht.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_error_invalidstate">
      <tuv xml:lang="en">
        <seg>Requested state for document is invalid.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Unbekannter Status für Dokument angefordert.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_success">
      <tuv xml:lang="en">
        <seg>The state of document %1$s was successfully changed to '%2$s'.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Dokument %1$s wurde erfolgreich in den Zustand '%2$s' gesetzt.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_deleted_success">
      <tuv xml:lang="en">
        <seg>Document was successfully deleted.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Das Dokument wurde erfolgreich gelöscht.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_removed_success">
      <tuv xml:lang="en">
        <seg>Document was successfully removed from the repository.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Das Dokument wurde erfolgreich aus dem Repository entfernt.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_published_success">
      <tuv xml:lang="en">
        <seg>Document %1$s published successfully!</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Das Dokument %1$s wurde freigeschaltet!</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_unpublished_success">
      <tuv xml:lang="en">
        <seg>Document %1$s successfully unpublished!</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Das Dokument %1$s wurde zurückgezogen!</seg>
      </tuv>
    </tu>

    <!-- TODO currently not used -->
    <tu tuid="publish_unlocking_date_not_reached">
      <tuv xml:lang="en">
        <seg>This document has an unlocking date, which has not been reached. It will be published automatically.</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Dieses Dokument hat ein festgelegtes Freischaltdatum, das noch nicht erreicht wurde. Es wird automatisch am festgelegten Tag freigeschaltet.</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_notification_headline">
      <tuv xml:lang="en">
        <seg>Notification Settings</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Benachrichtigungseinstellungen</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_notification_description">
      <tuv xml:lang="en">
        <seg>A notification about the publish event is sent to the following persons via email:</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Eine Benachrichtigung über das Freischalten wird per E-Mail an folgende Personen verschickt:</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_notification_submitter">
      <tuv xml:lang="en">
        <seg>Submitter</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Einsteller*in</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_notification_submitter_and_author">
      <tuv xml:lang="en">
        <seg>Submitter and %1$s. author</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Einsteller*in und %1$s. Autor*in</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_notification_author">
      <tuv xml:lang="en">
        <seg>Author</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Autor*in</seg>
      </tuv>
    </tu>

    <tu tuid="admin_workflow_notification_noemail">
      <tuv xml:lang="en">
        <seg>No email address available</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Keine E-Mail-Adresse verfügbar</seg>
      </tuv>
    </tu>


  </body>

</tmx>