<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tmx SYSTEM "http://www.gala-global.org/oscarStandards/tmx/tmx14.dtd">
<tmx version="1.4">

  <header creationtoolversion="1.0.0" datatype="winres" segtype="sentence" adminlang="en-us" srclang="de-de" o-tmf="abc" creationtool="XYZTool">
  </header>

  <body>

    <tu tuid="error_search_unavailable">
        <tuv xml:lang="en">
            <seg>The search service is currently not available.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Der Suchdienst ist im Moment nicht verfügbar.</seg>
        </tuv>
    </tu>

    <tu tuid="error_search_invalidquery">
        <tuv xml:lang="en">
            <seg>The given search query is not supported.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Die angegebene Suchanfrage wird nicht unterstützt.</seg>
        </tuv>
    </tu>

    <tu tuid="error_search_unknown">
        <tuv xml:lang="en">
            <seg>An unknown error occurred when trying to execute search.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Die Suche konnte aufgrund eines Fehlers nicht ausgeführt werden.</seg>
        </tuv>
    </tu>

    <tu tuid="error_page_not_found">
        <tuv xml:lang="en">
            <seg>Page not found</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Seite nicht gefunden</seg>
        </tuv>
    </tu>

    <tu tuid="error_msg_page_not_found">
        <tuv xml:lang="en">
            <seg>The accessed URL was not valid.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Die angegebende URL ist nicht gültig.</seg>
        </tuv>
    </tu>

    <tu tuid="error_application">
        <tuv xml:lang="en">
            <seg>Application error</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Anwendungsfehler</seg>
        </tuv>
    </tu>

    <tu tuid="error_uploaded_files">
        <!-- Text for alreaded uploaded files -->
        <tuv xml:lang="en">
            <seg>Error: File already exists or could not be stored.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Fehler: Datei exisitert bereits oder kann nicht gespeichert werden.</seg>
        </tuv>
    </tu>

    <tu tuid="wrong_identity_error">
        <!-- Text for alreaded uploaded files -->
        <tuv xml:lang="en">
            <seg>You need another identity to access this page.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Sie dürfen nicht auf diese Seite zugreifen.</seg>
        </tuv>
    </tu>

    <tu tuid="password_confirmation_error">
        <!-- Text for alreaded uploaded files -->
        <tuv xml:lang="en">
            <seg>Password confirmation does not match.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Die beiden Passworteingaben sind nicht identisch.</seg>
        </tuv>
    </tu>

    <tu tuid="fileUploadErrorNoFile">
        <tuv xml:lang="en">
            <seg>File upload is required.</seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>Sie haben keine Datei ausgewählt.</seg>
        </tuv>
    </tu>

      <!-- TODO move to TMX file for field hints? -->
      <tu tuid="admin_persons_values_not_trimmed">
          <tuv xml:lang="en">
              <seg>Some values have additional leading or trailing spaces. Please select update to trim values when saving.</seg>
          </tuv>
          <tuv xml:lang="de">
              <seg>Einige Werte haben zusätzliche Leerzeichen. Bitte Update auswählen für eine Bereinigung beim Speichern.</seg>
          </tuv>
      </tu>

      <tu tuid="identifierInvalidFormat">
          <tuv xml:lang="en">
              <seg>'%value%' is malformed.</seg>
          </tuv>
          <tuv xml:lang="de">
              <seg>'%value%' hat eine unerlaubte Form.</seg>
          </tuv>
      </tu>

      <tu tuid="identifierInvalidCheckdigit">
          <tuv xml:lang="en">
               <seg>The check digit of '%value%' is not valid.</seg>
          </tuv>
          <tuv xml:lang="de">
                <seg>Die Prüfziffer für '%value%' ist nicht korrekt.</seg>
          </tuv>
      </tu>

  </body>

</tmx>
