    <tu tuid="setup_message_error_read-access">
        <tuv xml:lang="en">
            <seg>
                '%1$s' is not readable.
                Please make sure that the web server has read access to the respective directory.
            </seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>
                '%1$s' kann nicht gelesen werden.
                Bitte stellen Sie sicher, dass der Webserver Leserechte für das entsprechende Verzeichnis hat.
            </seg>
        </tuv>
    </tu>

    <tu tuid="setup_message_error_write-access">
        <tuv xml:lang="en">
            <seg>
                '%1$s' is not writeable.
                Please make sure that the web server has write access to the respective directory.
            </seg>
        </tuv>
        <tuv xml:lang="de">
            <seg>
                '%1$s' kann nicht geschrieben werden.
                Bitte stellen Sie sicher, dass der Webserver Schreibrechte auf das entsprechende Verzeichnis hat.
            </seg>
        </tuv>
    </tu>

    <tu tuid="setup_message_error_filenotfound">
        <tuv xml:lang="en">
            <seg>
                The File '%1$s' is not readable or does not exist on the file system.
            </seg>
