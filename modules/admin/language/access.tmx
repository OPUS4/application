<?xml version="1.0" ?>
<!DOCTYPE tmx SYSTEM "http://www.lisa.org/fileadmin/standards/tmx1.4/tmx14.dtd.txt">
<tmx version="1.4">

<header creationtoolversion="1.0.0" datatype="winres" segtype="sentence" adminlang="en-us" srclang="de-de" o-tmf="abc" creationtool="XYZTool">
</header>

<body>
    <tu tuid="admin_access_listmodule">
      <tuv xml:lang="en">
        <seg>Access to Modules/Controllers</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Zugriff auf Module/Controller</seg>
      </tuv>
    </tu>

    <tu tuid="admin_access_listrole">
      <tuv xml:lang="en">
        <seg>Access to Document</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Zugriff auf Dokument</seg>
      </tuv>
    </tu>

    <tu tuid="access_select_module">
      <tuv xml:lang="en">
        <seg>Module access for role '%1$s'</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Modul-Zugriff für Rolle '%1$s'</seg>
      </tuv>
    </tu>

    <tu tuid="access_current_role_name">
      <tuv xml:lang="en">
        <seg>
        You are editing module access for role '%1$s'.  Please select
        modules you would like to grant access to.
        </seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>
        Sie bearbeiten gerade den Modul-Zugriff für Rolle '%1$s'.
        Bitte wählen Sie Module, für die Sie Zugriff gewähren möchten.
        </seg>
      </tuv>
    </tu>

    <tu tuid="access_button_save">
      <tuv xml:lang="en">
        <seg>Save</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Speichern</seg>
      </tuv>
    </tu>

    <tu tuid="access_button_cancel">
      <tuv xml:lang="en">
        <seg>Cancel</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Abbrechen</seg>
      </tuv>
    </tu>

    <tu tuid="access_redirect_message">
      <tuv xml:lang="en">
        <seg>Done</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Fertig</seg>
      </tuv>
    </tu>

    <tu tuid="access_submit_save">
      <tuv xml:lang="en">
        <seg>Operation complete</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Vorgang abgeschlossen</seg>
      </tuv>
    </tu>

    <tu tuid="access_submit_cancel">
      <tuv xml:lang="en">
        <seg>Operation canceled</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Vorgang abgebrochen</seg>
      </tuv>
    </tu>

    <tu tuid="access_save_message">
      <tuv xml:lang="en">
        <seg>Your changes have been successfully saved</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Ihre Änderungen wurden gespeichert</seg>
      </tuv>
    </tu>

    <tu tuid="access_cancel_message">
      <tuv xml:lang="en">
        <seg>Your changes have been discarded</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Ihre Änderungen wurden verworfen</seg>
      </tuv>
    </tu>

    <tu tuid="access_select_role">
      <tuv xml:lang="en">
        <seg>Please select roles you would like to grant access to this document</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Bitte wählen Sie die Rollen, die Zugriff auf dieses Dokument erhalten sollen</seg>
      </tuv>
    </tu>

    <tu tuid="access_current_docid">
      <tuv xml:lang="en">
        <seg>Currently selected document ID:</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Momentan ausgewählte Dokumenten-ID:</seg>
      </tuv>
    </tu>

    <tu tuid="access_available_roles">
      <tuv xml:lang="en">
        <seg>Available roles</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Verfügbare Rollen</seg>
      </tuv>
    </tu>

    <tu tuid="admin_access_checked_in_guest">
      <tuv xml:lang="en">
        <seg>(Access already granted by 'guest' role)</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>(Bereits erlaubt durch 'guest'-Rolle)</seg>
      </tuv>
    </tu>

    <tu tuid="access_module_changes">
      <tuv xml:lang="en">
        <seg>Changing Module Access</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Änderungen am Modul-Zugriff</seg>
      </tuv>
    </tu>

    <tu tuid="access_module_changes_affect_accounts">
      <tuv xml:lang="en">
        <seg>Changes will affect the following accounts</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Änderungen betreffen die folgenden Nutzerkonten</seg>
      </tuv>
    </tu>

</body>